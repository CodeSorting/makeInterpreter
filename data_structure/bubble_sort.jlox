함수 정렬(배열) {
    변수 n = 배열.길이;
    범위반복(변수 i=0; i<n-1; i=i+1) {
        범위반복(변수 j=0; j<n-i-1; j=j+1) {
            만약 (배열[j] > 배열[j+1]) {
                변수 tmp = 배열[j];
                배열[j] = 배열[j+1];
                배열[j+1] = tmp;
            }
        }
    }
}

변수 배열1 = [10,8,2,4,1,6,8,9,11];
정렬(배열1);
범위반복(변수 i=0;i<배열1.길이;i=i+1) {
    출력 배열1[i];
    출력 " ";
}