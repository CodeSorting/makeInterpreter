함수 이진탐색(배열, 찾는값) {
    변수 왼쪽 = 0;
    변수 오른쪽 = 배열.길이 - 1;
    조건반복 (왼쪽 <= 오른쪽) {
        변수 중간값 = (왼쪽 + 오른쪽) / 2;
        중간값 = (중간값 - 중간값 % 1); // 정수 내림
        만약 (배열[중간값] == 찾는값) {
            반환 중간값;
        } 아니면 만약 (배열[중간값] < 찾는값) {
            왼쪽 = 중간값 + 1;
        } 아니면 {
            오른쪽 = 중간값 - 1;
        }
    }
    반환 -1;
}

변수 a = [1, 3, 5, 7, 9, 11, 15, 18, 21, 22, 24, 25, 30, 32];
출력 이진탐색(a, 22);   // 9
출력 이진탐색(a, 11);   // 5